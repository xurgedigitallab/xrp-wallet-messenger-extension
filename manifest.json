{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "0.1.2",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "caches",
    "contextMenus",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://bidds.com/*",
    "https://xrp.cafe/*",
    "https://xmart.art/*",
    "https://xrpnft.com/*",
    "https://xpmarket.com/*",
    "https://sologenic.org/*",
    "https://app.zerpmon.world/*",
    "https://xrplexplorer.com/*",
    "https://xrpscan.com/*",
    "https://xrplwin.com/*",
    "https://neefty.io/*",
    "https://livenet.xrpl.org/*",
    "https://testnet.xrpl.org/*",
    "https://devnet.xrpl.org/*",
    "https://xahau.xrpl.org/*",
    "https://xahau-testnet.xrpl.org/*",
    "https://fuzion-xio.com/*",
    "https://artdept.fun/*",
    "https://firstledger.net/*",
    "https://xmagnetic.org/*",
    "http://localhost:3000/*"
  ],
  "background": {
    "service_worker": "dist/background.bundle.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icons/button_icon.svg"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}